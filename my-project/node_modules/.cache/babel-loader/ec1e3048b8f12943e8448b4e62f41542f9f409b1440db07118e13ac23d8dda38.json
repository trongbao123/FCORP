{"ast":null,"code":"import \"./Book.css\";\nimport { FetchBooks } from \"@/Api/BooksData\";\nimport { shortenDescription, shortenTitle } from \"@/Methods/Methods\";\nimport Loading from \"@/components/Loading/Loading.vue\";\nexport default {\n  name: \"ListBook\",\n  data() {\n    return {\n      books: [],\n      loading: true,\n      loadingMessage: \"Loading books...\"\n    };\n  },\n  async mounted() {\n    try {\n      const data = await FetchBooks();\n      this.books = data;\n    } catch (error) {\n      console.error(\"Error fetching books:\", error);\n    } finally {\n      setTimeout(() => {\n        this.loading = false;\n      }, 2000);\n    }\n  },\n  methods: {\n    shortenDescription,\n    shortenTitle\n  },\n  components: {\n    Loading\n  }\n};","map":{"version":3,"names":["FetchBooks","shortenDescription","shortenTitle","Loading","name","data","books","loading","loadingMessage","mounted","error","console","setTimeout","methods","components"],"sources":["C:\\Users\\Admin\\Desktop\\Test-FCORB-FE\\my-project\\src\\page\\Book\\Book.vue"],"sourcesContent":["<template>\r\n  <div class=\"container mx-auto p-4\">\r\n    <h1 class=\"title text-3xl bg-red-600 w-20 font-bold mb-4\">LIST OF BOOKS</h1>\r\n    <Loading :loading=\"loading\" :message=\"loadingMessage\" />\r\n    <div v-if=\"!loading\">\r\n      <div class=\"row\">\r\n        <div v-for=\"book in books\" :key=\"book.id\" class=\"col\">\r\n          <h2 class=\"font-sans\">\r\n            <router-link to=\"\">{{ shortenTitle(book._source.title) }}</router-link>\r\n          </h2>\r\n          <div class=\"Book-font\">\r\n            <p class=\"text-3xl font-bold underline\"><b>Author</b>: {{ book._source.author }}</p>\r\n            <p class=\"text-gray-700 mb-4\"><b>PublishedDate</b>: {{ book._source.publishedDate }}</p>\r\n            <p class=\"text-gray-700\">{{ shortenDescription(book._source.description) }}</p>\r\n          </div>\r\n          <div>\r\n            <p class=\"price\"><b>Price</b>: {{ book._source.price }}$</p>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport \"./Book.css\"\r\nimport { FetchBooks } from \"@/Api/BooksData\"\r\nimport { shortenDescription, shortenTitle } from \"@/Methods/Methods\"\r\nimport Loading from \"@/components/Loading/Loading.vue\"\r\n\r\nexport default {\r\n  name: \"ListBook\",\r\n  data() {\r\n    return {\r\n      books: [],\r\n      loading: true,\r\n      loadingMessage: \"Loading books...\"\r\n    }\r\n  },\r\n  async mounted() {\r\n    try {\r\n      const data = await FetchBooks()\r\n      this.books = data\r\n    } catch (error) {\r\n      console.error(\"Error fetching books:\", error)\r\n    } finally {\r\n      setTimeout(() => {\r\n        this.loading = false\r\n      }, 2000)\r\n    }\r\n  },\r\n  methods: {\r\n    shortenDescription,\r\n    shortenTitle\r\n  },\r\n  components: {\r\n    Loading\r\n  }\r\n}\r\n</script>\r\n"],"mappings":"AAyBA,OAAO,YAAW;AAClB,SAASA,UAAS,QAAS,iBAAgB;AAC3C,SAASC,kBAAkB,EAAEC,YAAW,QAAS,mBAAkB;AACnE,OAAOC,OAAM,MAAO,kCAAiC;AAErD,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE,IAAI;MACbC,cAAc,EAAE;IAClB;EACF,CAAC;EACD,MAAMC,OAAOA,CAAA,EAAG;IACd,IAAI;MACF,MAAMJ,IAAG,GAAI,MAAML,UAAU,CAAC;MAC9B,IAAI,CAACM,KAAI,GAAID,IAAG;IAClB,EAAE,OAAOK,KAAK,EAAE;MACdC,OAAO,CAACD,KAAK,CAAC,uBAAuB,EAAEA,KAAK;IAC9C,UAAU;MACRE,UAAU,CAAC,MAAM;QACf,IAAI,CAACL,OAAM,GAAI,KAAI;MACrB,CAAC,EAAE,IAAI;IACT;EACF,CAAC;EACDM,OAAO,EAAE;IACPZ,kBAAkB;IAClBC;EACF,CAAC;EACDY,UAAU,EAAE;IACVX;EACF;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}